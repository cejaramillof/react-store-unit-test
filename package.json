    "babel-plugin-react-remove-properties": "^0.3.0",
    "check-prop-types": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "jest-enzyme": "^7.1.2",
    "moxios": "^0.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "supertest": "^6.1.3"
  },
