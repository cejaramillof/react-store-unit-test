  "homepage": "",
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/store/__mocks__/styleMock.js",
