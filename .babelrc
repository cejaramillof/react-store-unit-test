{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-class-properties"
  ],
  "env": {
    "production": {
      "plugins": [
        [ "react-remove-properties", { "properties": [ "data-test" ] } ]
      ]
    },
    "test": {
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  }
}